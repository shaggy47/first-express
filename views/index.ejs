<% include header %>
<% if(entries.length > 0) { %>
    <% entries.forEach(function(entry){ %>
       <div class="panel panel-default">
            <div class="panel-heading">
                 <div class="text-muted pull-right">
                     <%= entry.published %>
                 </div>
                 <%=entry.title%>   
            </div>
            <div class="panel-body">
               <%=entry.body%>     
            </div>
      </div> 
   <% }) %>
   <% } else { %>
        No entries: <a href="/new-entry" class="btn btn-primary">Add one!</a>
   <% } %> 
   <%include footer%>